<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0" 
    xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" 
    xmlns="http://www.opengis.net/sld" 
    xmlns:ogc="http://www.opengis.net/ogc" 
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <NamedLayer>
    <Name>Color</Name>
    <UserStyle>
      <Title>color_gradient</Title>
      <FeatureTypeStyle>
        <Rule>
          <RasterSymbolizer>
            <ColorMap>
              <ColorMapEntry color="#641E16" quantity="5" />
              <ColorMapEntry color="#FF0000" quantity="10" />
              <ColorMapEntry color="#ff9900" quantity="15" />
              <ColorMapEntry color="#ffffff" quantity="20" />
              <ColorMapEntry color="#cccc00" quantity="40" />
              <ColorMapEntry color="#00ff00" quantity="60" />
              <ColorMapEntry color="#009900" quantity="75" />
            </ColorMap>
          </RasterSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

